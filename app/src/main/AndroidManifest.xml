<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.uta.cse5324.team1.teammanagementsystemandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ManagerMenuGUI" />
        <activity android:name=".LeadMenuGUI" />
        <activity android:name=".MemberMenuGUI" />
        <activity android:name=".UserMenuGUI" />
        <activity
            android:name=".JoinTeamList"
            android:parentActivityName=".UserMenu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserMenu" />
        </activity>
        <activity
            android:name=".CreateTeamForm"
            android:parentActivityName=".UserMenu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".UserMenu" />
        </activity>
        <activity android:name=".CreateProfileForm" />
        <activity android:name=".LoginForm" />
        <activity android:name=".CreateProjectForm">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>